# expect: 42
# Wildcard in variant patterns (uses stdlib Result)

isResultOk(Ok(_)) = true
isResultOk(Err(_)) = false

getOkOrDefault(Ok(x)) = x
getOkOrDefault(Err(_)) = -1

main() = {
    assert_eq(true, isResultOk(Ok(5)))
    assert_eq(false, isResultOk(Err("error")))
    assert_eq(10, getOkOrDefault(Ok(10)))
    assert_eq(-1, getOkOrDefault(Err("oops")))
    42
}
