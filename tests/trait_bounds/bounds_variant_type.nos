# expect: passed
type MyResult = MyOk(Int) | MyErr(String)

hashable[T: Hash](x: T) -> Int = hash(x)
equal[T: Eq](x: T, y: T) -> Bool = x == y

main() = {
    ok = MyOk(42)
    err = MyErr("error")
    h1 = hashable(ok)
    h2 = hashable(err)
    assert(h1 != h2)
    assert(equal(ok, MyOk(42)) == true)
    assert(equal(ok, err) == false)
    "passed"
}
