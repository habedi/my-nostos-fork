# expect_error: shadows built-in
# Test that trait method names cannot shadow builtins

trait Splittable
    split(self) -> (Int, Int)
end

Int: Splittable
    split(self) = (self, self)
end

splitAndSum[T: Splittable](x: T) -> Int = {
    (a, b) = x.split()
    a + b
}

main() = {
    result = splitAndSum(7)
    result + 1
}
