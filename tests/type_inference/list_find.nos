# expect: 8
# Find first element matching predicate

myFind[T](lst: List[T], pred: T -> Bool) = match lst {
    [] -> None()
    [x | xs] -> if pred(x) then Some(x) else myFind(xs, pred)
}

main() = {
    nums = [1, 3, 5, 8, 10, 12]
    result = myFind(nums, x => x % 2 == 0)
    optUnwrapOr(result, 0)
}
