# expect: 100
# Test generic with conditional logic

trait Evaluable
    evaluate(self) -> Int
end

Int: Evaluable
    evaluate(self) = self
end

processConditional[T: Evaluable](x: T, useDouble: Bool) -> Int = {
    base = x.evaluate()
    if useDouble then base * 2 else base
}

main() = processConditional(50, true)
