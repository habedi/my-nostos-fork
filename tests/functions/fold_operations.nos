# expect: 0
# Various fold operations - compute 9! = 362880

# Left fold
myFoldl(_, acc, []) = acc
myFoldl(f, acc, [h | t]) = myFoldl(f, f(acc, h), t)

# Right fold
myFoldr(_, acc, []) = acc
myFoldr(f, acc, [h | t]) = f(h, myFoldr(f, acc, t))

# Generate range
myRange(start, stop) = if start > stop then [] else [start | myRange(start + 1, stop)]

# Product using myFoldl
myProduct(lst) = myFoldl((a, b) => a * b, 1, lst)

# 9! = 362880
main() = {
    assert_eq(362880, myProduct(myRange(1, 9)))
    0
}
